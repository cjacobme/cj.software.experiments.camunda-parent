  <bpmn:process id="Process_LoanRequest" name="Loan Request" isExecutable="true" camunda:candidateStarterGroups="Indian">
    <bpmn:documentation>This process has to be executed when a loan was requested</bpmn:documentation>
    <bpmn:startEvent id="StartEvent_LoanRequest" name="Loan Requested&#10;&#10;">
      <bpmn:outgoing>SequenceFlow_1phke8d</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:sequenceFlow id="SequenceFlow_1phke8d" sourceRef="StartEvent_LoanRequest" targetRef="UserTask_EnterLoanData" />
    <bpmn:userTask id="UserTask_EnterLoanData" name="Enter Loan Data">
      <bpmn:incoming>SequenceFlow_1phke8d</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_18kyild</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="ExclusiveGateway_ToBeApproved" name="to be approved?" default="SequenceFlow_approve">
      <bpmn:incoming>SequenceFlow_18kyild</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_approve</bpmn:outgoing>
      <bpmn:outgoing>SequenceFlow_NoApprove</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="SequenceFlow_18kyild" sourceRef="UserTask_EnterLoanData" targetRef="ExclusiveGateway_ToBeApproved" />
    <bpmn:sequenceFlow id="SequenceFlow_approve" name="yes" sourceRef="ExclusiveGateway_ToBeApproved" targetRef="UserTask_ApproveLoan" />
    <bpmn:userTask id="UserTask_ApproveLoan" name="Approve Loan" camunda:candidateGroups="Chief">
      <bpmn:incoming>SequenceFlow_approve</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_1hekpbp</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="ExclusiveGateway_1wuus9n">
      <bpmn:incoming>SequenceFlow_NoApprove</bpmn:incoming>
      <bpmn:incoming>SequenceFlow_Approved</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_1lxc6aw</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="SequenceFlow_NoApprove" name="no" sourceRef="ExclusiveGateway_ToBeApproved" targetRef="ExclusiveGateway_1wuus9n">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${requestedLoan &lt;= 100}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:exclusiveGateway id="ExclusiveGateway_LoanApproved" name="Approved?" default="SequenceFlow_1t68b4v">
      <bpmn:incoming>SequenceFlow_1hekpbp</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_Approved</bpmn:outgoing>
      <bpmn:outgoing>SequenceFlow_1t68b4v</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="SequenceFlow_1hekpbp" sourceRef="UserTask_ApproveLoan" targetRef="ExclusiveGateway_LoanApproved" />
    <bpmn:sequenceFlow id="SequenceFlow_Approved" name="approved" sourceRef="ExclusiveGateway_LoanApproved" targetRef="ExclusiveGateway_1wuus9n">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${approved}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:endEvent id="EndEvent_Rejected" name="Rejected">
      <bpmn:incoming>SequenceFlow_1hhc0bk</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="SequenceFlow_1t68b4v" name="Rejected" sourceRef="ExclusiveGateway_LoanApproved" targetRef="Task_107zr15" />
    <bpmn:endEvent id="EndEvent_Accepted" name="Loan accepted">
      <bpmn:incoming>SequenceFlow_07clrhu</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="SequenceFlow_1lxc6aw" name="Approved" sourceRef="ExclusiveGateway_1wuus9n" targetRef="Task_SendMoney" />
    <bpmn:sequenceFlow id="SequenceFlow_07clrhu" sourceRef="Task_SendMoney" targetRef="EndEvent_Accepted" />
    <bpmn:serviceTask id="Task_SendMoney" name="Send Money" camunda:class="cj.experiments.camunda._13_vaadin.delegate.LoggerDelegate">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="logMessage">Send ${requestedLoan}</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>SequenceFlow_1lxc6aw</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_07clrhu</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="SequenceFlow_1hhc0bk" sourceRef="Task_107zr15" targetRef="EndEvent_Rejected" />
    <bpmn:serviceTask id="Task_107zr15" name="Send Rejection Mail" camunda:class="cj.experiments.camunda._13_vaadin.delegate.LoggerDelegate">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="logMessage">Requested Loan ${requestedLoan} was rejected</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>SequenceFlow_1t68b4v</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_1hhc0bk</bpmn:outgoing>
    </bpmn:serviceTask>
  </bpmn:process>
